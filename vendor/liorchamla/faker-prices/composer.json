{
  "name": "liorchamla/faker-prices",
  "description": "Provides typical and credible prices for Faker (fzaninotto/faker or fakerphp/faker) library",
  "type": "library",
  "minimum-stability": "stable",
  "keywords": [
    "faker",
    "fixtures",
    "prices"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Lior Chamla",
      "email": "contact@web-develop.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Liior\\Faker\\": "src/Faker"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Liior\\Tests\\Faker\\": "tests/Faker"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "pestphp/pest": "^0.3.19",
    "fakerphp/faker": "^1.13"
  }
}
